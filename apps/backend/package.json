{
  "name": "souqsyria-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "seed": "ts-node src/testing/scripts/run-seed.ts",
    "seed:basic": "ts-node src/testing/scripts/run-seed.ts basic",
    "seed:full": "ts-node src/testing/scripts/run-seed.ts full",
    "seed:syrian": "ts-node src/testing/scripts/run-seed.ts syrian",
    "seed:performance": "ts-node src/testing/scripts/run-seed.ts performance",
    "seed:realistic": "ts-node src/testing/scripts/run-seed.ts realistic",
    "seed:clean": "ts-node src/testing/scripts/run-seed.ts clean",
    "seed:help": "ts-node src/testing/scripts/run-seed.ts --help",
    "seed:simple": "ts-node src/testing/scripts/simple-seed.ts",
    "seed:simple:basic": "ts-node src/testing/scripts/simple-seed.ts basic",
    "seed:simple:roles": "ts-node src/testing/scripts/simple-seed.ts roles",
    "seed:simple:users": "ts-node src/testing/scripts/simple-seed.ts users",
    "seed:simple:clean": "ts-node src/testing/scripts/simple-seed.ts clean",
    "seed:simple:stats": "ts-node src/testing/scripts/simple-seed.ts stats",
    "test:cart-performance": "ts-node scripts/test-cart-performance.ts",
    "test:cart-performance:light": "ts-node scripts/test-cart-performance.ts --concurrent 100 --duration 30",
    "test:cart-performance:heavy": "ts-node scripts/test-cart-performance.ts --concurrent 500 --duration 60",
    "test:cart-security": "jest --config ./test/jest-integration.json cart-security.integration.spec.ts",
    "test:cart-guards": "jest --testPathPattern=guards.*spec.ts",
    "validate:cart-security-simple": "ts-node scripts/validate-cart-security-simple.ts",
    "validate:cart-security": "ts-node scripts/validate-cart-security.ts",
    "seed:admin-roles": "ts-node src/scripts/seed-owner-admin-roles.ts",
    "seed:test-users": "ts-node src/scripts/seed-test-users.ts",
    "seed:routes": "ts-node src/scripts/seed-routes.ts",
    "seed:routes:overwrite": "ts-node src/scripts/seed-routes.ts --overwrite",
    "seed:routes:report": "ts-node src/scripts/seed-routes.ts --report",
    "seed:routes:validate": "ts-node src/scripts/seed-routes.ts --validate"
  },
  "dependencies": {
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^10.3.5",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.3.5",
    "@nestjs/event-emitter": "^2.1.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.22",
    "@nestjs/schedule": "^6.0.0",
    "@nestjs/swagger": "^8.1.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@sentry/node": "^9.30.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^2.0.0",
    "@types/nodemailer": "^7.0.5",
    "@types/passport-facebook": "^3.0.4",
    "@types/passport-google-oauth20": "^2.0.17",
    "axios": "^1.9.0",
    "bcryptjs": "^3.0.3",
    "cache-manager": "^7.2.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "firebase-admin": "^13.3.0",
    "multer": "^2.0.2",
    "mysql2": "^3.14.0",
    "nodemailer": "^7.0.12",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.3",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.22",
    "uuid": "^10.0.0",
    "winston": "^3.17.0",
    "winston-elasticsearch": "^0.19.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.6",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.18",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.4",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "\\.e2e\\.spec\\.ts$",
      "cart/integration/",
      "mobile/__tests__/"
    ],
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
