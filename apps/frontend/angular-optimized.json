{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "souq-syria-storefront": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss",
          "changeDetection": "OnPush"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/souq-syria-storefront",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "serviceWorker": "ngsw-config.json",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              },
              {
                "glob": "*.{png,jpg,jpeg,webp,svg}",
                "input": "src/assets/images",
                "output": "/assets/images"
              },
              {
                "glob": "**/*.woff2",
                "input": "src/assets/fonts",
                "output": "/assets/fonts"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "webWorkers": {
              "enable": true
            }
          },
          "configurations": {
            "production": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "fonts": {
                  "inline": true
                }
              },
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "aot": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "400kB",
                  "maximumError": "800kB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kB",
                  "maximumError": "4kB"
                },
                {
                  "type": "bundle",
                  "name": "vendor",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "bundle",
                  "name": "main",
                  "maximumWarning": "300kB",
                  "maximumError": "600kB"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "serviceWorker": true
            },
            "production-analyze": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": true,
                  "inlineCritical": true
                },
                "fonts": {
                  "inline": true
                }
              },
              "outputHashing": "all",
              "sourceMap": true,
              "namedChunks": true,
              "extractLicenses": true,
              "vendorChunk": true,
              "buildOptimizer": true,
              "aot": true,
              "statsJson": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "400kB",
                  "maximumError": "800kB"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "serviceWorker": true
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true,
              "vendorChunk": true,
              "namedChunks": true,
              "buildOptimizer": false
            },
            "performance": {
              "optimization": {
                "scripts": true,
                "styles": {
                  "minify": false,
                  "inlineCritical": false
                }
              },
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true,
              "namedChunks": true,
              "buildOptimizer": true,
              "aot": true,
              "statsJson": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "souq-syria-storefront:build:production"
            },
            "development": {
              "buildTarget": "souq-syria-storefront:build:development"
            },
            "performance": {
              "buildTarget": "souq-syria-storefront:build:performance"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": []
          }
        },
        "bundle-analyze": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/souq-syria-storefront-analyze",
            "index": "src/index.html",
            "browser": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ],
            "styles": [
              "src/styles.scss"
            ],
            "scripts": [],
            "sourceMap": true,
            "namedChunks": true,
            "vendorChunk": true,
            "statsJson": true,
            "buildOptimizer": true,
            "aot": true
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false,
    "cache": {
      "enabled": true
    },
    "completion": {
      "prompted": false
    }
  }
}